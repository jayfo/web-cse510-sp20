{
  "name": "angular-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run compile:markdown && ng serve",
    "build": "npm run compile:markdown && ng build",
    "test": "npm run compile:markdown && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:client-and-server-bundles": "ng build --prod && ng run angular-workspace:server:production",
    "build:prerender": "npm run compile:markdown && npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "build:ssr": "npm run compile:markdown && npm run build:client-and-server-bundles && npm run compile:server",
    "compile:markdown": "tsc -p tsconfig.markdown.json && node dist/markdown.js",
    "compile:server": "tsc -p tsconfig.server.json",
    "generate:prerender": "cd dist && node prerender",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.2",
    "@angular/common": "^8.2.2",
    "@angular/compiler": "^8.2.2",
    "@angular/core": "^8.2.2",
    "@angular/forms": "^8.2.2",
    "@angular/platform-browser": "^8.2.2",
    "@angular/platform-browser-dynamic": "^8.2.2",
    "@angular/platform-server": "^8.2.2",
    "@angular/router": "^8.2.2",
    "@ng-bootstrap/ng-bootstrap": "^5.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.1",
    "bootstrap": "~4.3.1",
    "core-js": "^3.2.1",
    "font-awesome": "~4.7.0",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "rxjs": "~6.5.2",
    "ts-loader": "^6.0.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.2",
    "@angular/cli": "^8.2.2",
    "@angular/compiler-cli": "^8.2.2",
    "@angular/language-service": "^8.2.2",
    "@types/jasmine": "^3.4.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/markdown-it": "0.0.8",
    "@types/markdown-it-anchor": "^4.0.3",
    "@types/node": "~12.7.2",
    "codelyzer": "^5.1.0",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "markdown-it": "^9.1.0",
    "markdown-it-anchor": "^5.2.4",
    "protractor": "^5.4.2",
    "ts-node": "~8.3.0",
    "tslint": "~5.19.0",
    "typescript": "~3.5.3"
  }
}
